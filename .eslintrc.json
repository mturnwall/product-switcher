/**
 * Rules have 3 settings 0 (off), 1 (warning), 2 (error)
 */
{
  "extends": "mirum",
  "parserOptions": {
    "table": true
  },
  "rules": {
    "no-new": "off",
    "dot-location": ["error", "property"],
    "arrow-parens": ["error", "as-needed"],
    "object-curly-newline": ["error", {
      "minProperties": 1
    }]
   }
}
